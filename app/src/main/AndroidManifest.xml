<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appfetin">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppFetin">
        <activity android:name=".activity.ProdutosActivity"
            android:parentActivityName=".activity.ClienteActivity" />
        <activity android:name=".activity.CadastroEmpresaActivity" />
        <activity
            android:name=".activity.NovoProdutoActivity"
            android:parentActivityName=".activity.EmpresaActivity" />
        <activity
            android:name=".activity.EditarEmpresaActivity"
            android:parentActivityName=".activity.EmpresaActivity" />
        <activity
            android:name=".activity.EditarClienteActivity"
            android:parentActivityName=".activity.ClienteActivity" />
        <activity android:name=".activity.EmpresaActivity" />
        <activity android:name=".activity.ClienteActivity" />
        <activity android:name=".activity.CadastroActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>